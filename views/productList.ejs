    </div>
    <div class="deals"><img src="images/deals.jpg" alt="" class="img-responsive"></div>
</div>
<div class="right-aside">
    <% for(var i=0;i<data.length;i++){%>
        <div class="pro-listing">
            <div class="item1">
                <div class="product-thumb">
                    <div class="pro-img">
                        <img src="images/pro1.jpg" alt="">
                    </div>	
                    <div class="butns">
                        <a href="" class="view view1">View</a>
                        <a href="" class="view view1">Buy Now</a>
                    </div>
                </div>
            <div class="des-title">
                <%= data[i].name %>
            </div>
            <div class="rate">
                <span class="fa fa-inr rupee"></span><%= data[i].price%>
            </div>
        </div>
    <%}%>

</div>