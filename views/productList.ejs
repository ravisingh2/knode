<div class="left-aside">
    <ol class="breadcrumb">
        <li><i class="fa fa-home home-icon"></i></li>
        <li class="active">Product-Listing</li>
    </ol>
    <div class="category kkk">
        <h4 class="shop">Shop By</h4>
        <h5>Categories</h5>
        <ul class="cat">
            <li><input type="checkbox"><span class="cat-name">Brass</span><span class="number">12</span></li>
            <li><input type="checkbox"><span class="cat-name">Handmade Bags</span><span class="number">12</span></li>
            <li><input type="checkbox"><span class="cat-name">Statues</span><span class="number">12</span></li>
            <li><input type="checkbox"><span class="cat-name">Sculptures</span><span class="number">12</span></li>
            <li><input type="checkbox"><span class="cat-name">Photo Frames</span><span class="number">12</span></li>
        </ul>
